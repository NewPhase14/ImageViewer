<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox spacing="14.0" styleClass="image-background" stylesheets="@../css/style.css" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.GUI.Controller.ImageContainerController">
   <children>
      <VBox alignment="CENTER">
         <children>
            <Button mnemonicParsing="false" prefHeight="140.0" prefWidth="190.0" styleClass="image-buttons">
               <graphic>
                  <ImageView fx:id="imgPicture" fitHeight="150.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@../icons/Picture.png" />
                     </image>
                  </ImageView>
               </graphic>
            </Button>
            <Label fx:id="txtTitle" />
         </children>
      </VBox>
   </children>
</HBox>
